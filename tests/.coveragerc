[run]
source = 
    services/
    strategies/
    utils/
    trade/
    backtest/
    data/
    auth/
    
omit = 
    */__init__.py
    */tests/*
    */migrations/*
    */schemas/*
    */models/*
    */config.py

[report]
show_missing = True
skip_covered = True
fail_under = 90
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError